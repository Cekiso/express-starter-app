<h1>Names</h1>

<div class="space_below">
	<a href="/add">Add pizza</a>
</div>
Name count: {{names.length}}

{{code}}

<table>
	<tr>
		<th>First name</th>
		<th>Last name</th>
	</tr>

	{{#each names}}
	<tr>
		<td>
			{{firstName}}
		</td>
		<td>
			{{lastName}}
		</td>
	</tr>
	{{/each}}
	<td>
		<a href="/edit">Edit</a>

	</td>
</table>